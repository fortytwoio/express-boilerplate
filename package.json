{
  "name": "express-boilerplate",
  "description": "Express Boilerplate project for all kind of web applications or REST APIs",
  "author": "Thomas Fritz <fritztho@gmail.com>",
  "homepage": "https://github.com/thomasfr/express-boilerplate",
  "private": true,
  "engines": {
    "node": ">=0.10.25 <=0.12"
  },
  "version": "0.0.3",
  "main": "index.js",
  "dependencies": {
    "async": "^0.9.0",
    "body-parser": "^1.2.2",
    "coffee-script": "^1.7.0",
    "cookie-parser": "^1.1.0",
    "cookie-session": "^1.0.2",
    "debug": "^0.8.1",
    "errorhandler": "^1.0.0",
    "express": "^4.3.2",
    "jade": "^1.3.0",
    "jquery": "^2.1.1",
    "loadfiles": "^0.1.1",
    "lodash": "^2.4.0",
    "morgan": "^1.1.1",
    "static-favicon": "^1.0.2"
  },
  "devDependencies": {
    "browserify": "^4.1.6",
    "browserify-shim": "^3.5.0",
    "coffeeify": "^0.6.0",
    "gulp": "^3.6.2",
    "gulp-changed": "^0.3.0",
    "gulp-concat": "^2.2.0",
    "gulp-imagemin": "^0.5.1",
    "gulp-livereload": "^1.5.0",
    "gulp-nodemon": "^1.0.3",
    "gulp-notify": "^1.3.1",
    "gulp-open": "^0.2.8",
    "nodemon": "^1.0.0",
    "pretty-hrtime": "^0.2.1",
    "vinyl-source-stream": "^0.1.1",
    "watchify": "^0.10.2"
  },
  "browser": {},
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "scripts": {
    "start": "node ./",
    "dev": "NODE_ENV=development DEBUG_COLORS=y nodemon --ext js,coffee,jade,json --ignore tmp/ --ignore var/ --ignore app/public/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thomasfr/express-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/thomasfr/express-boilerplate/issues"
  }
}
